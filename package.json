{
  "name": "simulabra",
  "version": "0.0.5",
  "repository": "simulabra/simulabra",
  "type": "module",
  "exports": {
    ".": "./src/base.js",
    "./html": "./src/html.js"
  },
  "module": "./src/base.js",
  "scripts": {
    "watchtest": "watchexec -w . -e js -r -- bun run src/runner.js",
    "test": "bun run src/runner.js",
    "test-ui": "bun run src/runner.js tests/ui",
    "test-bin": "bun run src/runner.js tests/bin",
    "agent": "watchexec -w . -e js -r -- bun run src/agent.js",
    "serve": "bun run src/agent.js"
  },
  "devDependencies": {
    "playwright": "latest"
  }
}
